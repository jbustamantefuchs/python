    {
        "id": "990f4c32aaa36db0",
        "type": "function",
        "z": "d20c8a1b93301854",
        "name": "function 2",
        "func": "// Si el topic es \"rested\", retorna 0 inmediatamente\nif (msg.topic === \"rested\") {\n    msg.payload = 0;\n    return msg;\n}\n\n// Suponemos que el Ã¡ngulo se encuentra en msg.payload.angle.degree\nvar angle = msg.payload.angle.degree;\nvar salida;\n\nif (angle >= 330 || angle <= 30) {\n    salida = 5; //right\n} else if (angle >= 31 && angle <= 59) {\n    salida = 1; // right forward\n} else if (angle >= 60 && angle <= 120) {\n    salida = 8; //forward\n} else if (angle >= 121 && angle <= 149) {\n    salida = 3; // left forward\n} else if (angle >= 150 && angle <= 210) {\n    salida = 6; //left\n} else if (angle >= 211 && angle <= 239) {\n    salida = 4; //left baward\n} else if (angle >= 240 && angle <= 300) {\n    salida = 7; // backward\n} else if (angle >= 301 && angle <= 329) {\n    salida = 2; // right backward\n} else {\n    salida = 0; // Caso imprevisto\n}\n\nmsg.payload = salida;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
